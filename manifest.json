{
    "name": "My Mobile PWA",
    "short_name": "PWA App",
    "description": "A Progressive Web App for mobile",
    "start_url": "/index.html",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#007bff",
    "icons": [
      {
        "src": "icon.jpeg",
        "sizes": "192x192",
        "type": "image/jpeg"
      },
      {
        "src": "icon.jpeg",
        "sizes": "512x512",
        "type": "image/jpeg"
      }
    ],
    "scope": "/",
    "orientation": "portrait",
    "serviceworker": {
      "src": "/service-worker.js",
      "scope": "/",
      "update_via_cache": "all"
    }
  }
  